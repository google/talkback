<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!-- To aid tooling, any string-containing xml file in this project should have a filename beginning with "strings" -->
<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!--
      Source: String spoken when a user as selected some characters in a password
      field, indicating the start and end point of the selection cursor. The word "characters" in
      this string means letters, numbers, or other symbols, NOT a person.
      Description: This is spoken to the user. [CHAR_LIMIT=NONE]
     -->
    <string name="template_password_selected">Characters <xliff:g id="from_index">%1$d</xliff:g> to <xliff:g id="to_index">%2$d</xliff:g></string>
    <!--
      Source: String spoken when a user has moved the text cursor inside a password field and the
      actual characters in the field are unknown. The word "character" in this string means a
      letter, number, or other symbol, NOT a person.
      Description: This is spoken to the user. [CHAR_LIMIT=NONE]
     -->
    <string name="template_password_traversed">Character <xliff:g id="index">%1$d</xliff:g></string>

    <!-- Window title used for announcing untitled window. -->
    <string name="untitled_window">untitled</string>

    <!--
          Source: An utterance template for announcing copy of text (during edit).
          Description: This is spoken to the user. [CHAR LIMIT="NONE"]
        -->
    <string name="template_text_copied">copied, <xliff:g id="text">%1$s</xliff:g></string>
    <!--
      Source: An utterance template for announcing a single upper-case (capital) letter.
      Description: This is spoken to the user. May not be translatable for some languages. [CHAR LIMIT="NONE"]

      TODO: This string was moved to compositor's resource. We can remove it later.
    -->
    <string name="template_capital_letter">capital <xliff:g id="uppercase_character">%1$s</xliff:g></string>
    <!-- Replacement string used when collapsing multiple repeated characters which is spoken to the user.  For example, in the case where we're about to speak 10 equal signs, we use this string to format the phrase "10 equal sign" rather than speaking "equal sign equal sign equal sign..." -->
    <string name="character_collapse_template">\u0020<xliff:g id="number" example="7">%1$d</xliff:g> <xliff:g id="character" example="equal sign">%2$s</xliff:g>      </string>
    <!--
      Source: An utterance template for announcing the current text-to-speech engine.
      Description: This is spoken to the user. [CHAR LIMIT="NONE"]
    -->
    <string name="template_current_tts_engine">Using <xliff:g id="engine_name">%1$s</xliff:g></string>
    <!-- Announced when the user changes reading mode to read an entire object at a time. -->
    <string name="granularity_default">Default</string>
    <!-- Announced when the user changes reading mode to read a single character at a time. -->
    <string name="granularity_character">Characters</string>
    <!-- Announced when the user changes reading mode to read a single word at a time. -->
    <string name="granularity_word">Words</string>
    <!-- Announced when the user changes reading mode to read a single line at a time. -->
    <string name="granularity_line">Lines</string>
    <!-- Announced when the user changes reading mode to read a single paragraph at a time. -->
    <string name="granularity_paragraph">Paragraphs</string>
    <!-- Announced when the user changes reading mode to read a single container at a time. [CHAR_LIMIT=NONE] -->
    <string name="granularity_container">Containers</string>
    <!-- Announced when the user changes reading mode to read a single window at a time. [CHAR_LIMIT=NONE] -->
    <string name="granularity_window">Windows</string>
    <!-- Announced when the user changes reading mode in web content to read a single section at a time. -->
    <string name="granularity_web_landmark">Landmarks</string>
    <!-- Announced when the user changes reading mode in web content to read a single section header at a time. -->
    <string name="granularity_web_heading">Headings</string>
    <!-- Announced when the user changes reading mode in web content to read a single list at a time. -->
    <string name="granularity_web_list">Lists</string>
    <!-- Announced when the user changes reading mode in web content to read a single link at a time. -->
    <string name="granularity_web_link">Links</string>
    <!-- Announced when the user changes reading mode in web content to read a single control at a time.
      A control is an element on a web page which is either a form field (buttons, text fields, combo boxes, etc) or a link.
    -->
    <string name="granularity_web_control">Controls</string>
    <!-- Announced when the user enters into navigating specialized web content such as tables or math. -->
    <string name="granularity_pseudo_web_special_content">Special content</string>
    <!-- Announced when the user changes navigation mode in native views to navigating a single section header at a time -->
    <string name="granularity_native_heading">Headings</string>
    <!-- Announced when the user changes navigation mode in native views to navigating a single control at a time.
      A control is an element on the screen which is a form field (buttons, text fields, combo boxes, etc).
      Currently only buttons and edit fields are considered.
    -->
    <string name="granularity_native_control">Controls</string>
    <!-- Announced when the user changes navigation mode in native views to navigating a single link at a time. -->
    <string name="granularity_native_link">Links</string>

    <!-- Announced for overlay window, example: picture-in-picture video. -->
    <string name="template_overlay_window"><xliff:g id="application_window_title" example="Application A">%1$s</xliff:g> picture in picture</string>
    <!-- Announced for the split screen mode in portrait screen. -->
    <string name="template_split_screen_mode_portrait"><xliff:g name="window_title_a" example="Application A">%1$s</xliff:g> on top, <xliff:g name="window_title_b" example="Application B">%2$s</xliff:g> on bottom</string>
    <!-- Announced for split screen mode in Left-to-Right landscape screen. -->
    <string name="template_split_screen_mode_landscape_ltr"><xliff:g name="window_title_a" example="Application A">%1$s</xliff:g> on left, <xliff:g name="window_title_b" example="Application B">%2$s</xliff:g> on right</string>
    <!-- Announced for split screen mode in Right-to-Left landscape screen. -->
    <string name="template_split_screen_mode_landscape_rtl"><xliff:g name="window_title_a" example="Application A">%1$s</xliff:g> on right, <xliff:g name="window_title_b" example="Application B">%2$s</xliff:g> on left</string>

    <!--
        Source: An utterance template for announcing the visible range of items within a list view.
        Description: This is spoken to the user. [CHAR LIMIT="NONE"]
    -->
    <!-- TODO: This template is used by both ScrollFeedbackManager and Compositor, clean up dependency if we move this string. -->
    <string name="template_scroll_from_to_count">Showing items <xliff:g id="from_index">%1$d</xliff:g> to <xliff:g id="to_index">%2$d</xliff:g> of <xliff:g id="total_count">%3$d</xliff:g>.</string>
    <!--
        Source: An utterance template for announcing the visible item within a list view.
        Description: This is spoken to the user. [CHAR LIMIT="NONE"]
    -->
    <!-- TODO: This template is used by both ScrollFeedbackManager and Compositor, clean up dependency if we move this string. -->
    <string name="template_scroll_from_count">Showing item <xliff:g id="from_index">%1$d</xliff:g> of <xliff:g id="total_count">%2$d</xliff:g>.</string>
    <!--
        Source: An utterance template for announcing the visible page inside a view pager.
        This is used if the view pager's current page does not have a content description.
        Description: This is spoken to the user. [CHAR LIMIT="NONE"]
    -->
    <!-- TODO: This template is used by both ScrollFeedbackManager and Compositor, clean up dependency if we move this string. -->
    <string name="template_viewpager_index_count">Page <xliff:g id="page_index">%1$d</xliff:g> of <xliff:g id="total_count">%2$d</xliff:g></string>
    <!--
        Source: A shorter utterance template for announcing the visible page inside a view pager.
        This is used if the view pager's current page has a content description.
        Description: This is spoken to the user. [CHAR LIMIT="NONE"]
    -->
    <!-- TODO: This template is used by both ScrollFeedbackManager and Compositor, clean up dependency if we move this string. -->
    <string name="template_viewpager_index_count_short"><xliff:g id="page_index">%1$d</xliff:g> of <xliff:g id="total_count">%2$d</xliff:g></string>

    <!--
        Template for a row in a dialog containing information on a URL. The dialog_text is the
        hyperlinked text visible to the user before the dialog is opened, and dialog_url is the URL
        pathway. This string should not be used if dialog_text equals dialog_url. The text will be
        truncated if the text exceeds more than one line on the dialog.
    -->
    <string name="url_dialog_table"><xliff:g id="dialog_text" example="Google">%1$s</xliff:g> (<xliff:g id="dialog_url" example="www.google.com">%2$s</xliff:g>)</string>

    <!-- Label for exit button for a scrim.[CHAR_LIMIT=NONE] -->
    <string name="exit_scrim">
        Exit
    </string>

    <!-- Announced when the window shows.[CHAR_LIMIT=NONE]-->
    <string name="show_keyboard_window">Showing <xliff:g id="window_title" example="Gboard">%1$s</xliff:g></string>
    <!-- Announced when the window hidden.[CHAR_LIMIT=NONE] -->
    <string name="hide_keyboard_window">keyboard hidden</string>

    <!-- Announced when toggle voice feedback is on.[CHAR_LIMIT=NONE] -->
    <string name="function_on">Spoken feedback is on</string>
    <!-- Announced when toggle voice feedback is off.[CHAR_LIMIT=NONE] -->
    <string name="function_off">Spoken feedback is off</string>
</resources>
