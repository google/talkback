<?xml version="1.0" encoding="utf-8"?>
<resources>
  <!-- Don’t show this again checkbox. [CHAR_LIMIT=30] -->
  <string name="dont_show_again">Don’t show this again</string>
  <!-- Done button. [CHAR_LIMIT=20] -->
  <string name="done">Done</string>
  <!-- Audial announcement that plays when number indicator is pressed. [CHAR_LIMIT=30] -->
  <string name="number_announcement">Number</string>
  <!-- Audial announcement that plays when capitalize indicator is pressed. [CHAR_LIMIT=30] -->
  <string name="capitalize_announcement">Cap</string>
  <!-- Audial announcement that plays when deleting text. [CHAR_LIMIT=30] -->
  <string name="read_out_deleted">%s deleted</string>
  <!-- Audial announcement that plays when typing indicators. 'dot' means braille dot. [CHAR_LIMIT=8] -->
  <plurals name="braille_dots">
    <item quantity="one">dot %s</item>
    <item quantity="other">dots %s</item>
  </plurals>
  <!-- The comma used for conjoining multiple language names into a list. In English, we use a comma followed by a space; other languages might use a comma on its own with no space (such as the stand-alone Chinese comma). This will be used to separate language names, resulting in phrases such as "Spanish, Arabic, French". [CHAR_LIMIT=5]-->
  <string name="language_split_comma">,\u0020</string>
  <!-- Language list preference item: this is a proper name for a certain
       braille coding standard that is common in English-speaking countries.
       While it's permissible to translate this term into the local language if
       such a translation already exists, we expect that many languages will use
        the English term. [CHAR_LIMIT=50] -->
  <string name="code_user_facing_name_ueb1">Unified English Braille Uncontracted</string>
  <!-- Language list preference item: this is a proper name for a certain
       braille coding standard that is common in English-speaking countries.
       While it's permissible to translate this term into the local language if
       such a translation already exists, we expect that many languages will use
        the English term. [CHAR_LIMIT=50] -->
  <string name="code_user_facing_name_ueb2">Unified English Braille Contracted</string>
  <!-- Language list preference item for the Spanish language. [CHAR_LIMIT=40] -->
  <string name="code_user_facing_name_es">Spanish</string>
  <!-- Language list preference item for the Arabic language. [CHAR_LIMIT=40] -->
  <string name="code_user_facing_name_ar">Arabic</string>
  <!-- Language list preference item for the French language. [CHAR_LIMIT=40] -->
  <string name="code_user_facing_name_fr">French</string>
  <!-- Language list preference item for the Polish language. [CHAR_LIMIT=40] -->
  <string name="code_user_facing_name_pl">Polish</string>


  <!-- Layout Options -->
  <!-- Screen-away. [CHAR_LIMIT=45] -->
  <string name="screen_away">Screen-away mode</string>
  <!-- Screen-away description. [CHAR_LIMIT=110] -->
  <string name="screen_away_detail">Hold device with screen away from you (screen-away mode)</string>
  <!-- Tabletop. [CHAR_LIMIT=45] -->
  <string name="tabletop">Tabletop mode</string>
  <!-- Tabletop description. [CHAR_LIMIT=110] -->
  <string name="tabletop_detail">Place device flat on a table (tabletop mode)</string>
  <!-- Auto detect. [CHAR_LIMIT=50] -->
  <string name="auto_detect">Adapt to how you hold the device</string>
</resources>
