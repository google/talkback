# match %# * %# 5-236
display + 346
math + 5-235
display , 6
punctuation , 2
decpoint , 256
punctuation - 36
math - 5-36
hyphen - 36
display . 46
punctuation . 256
decpoint . 3
display / 34
sign / 34
math / 5-34
punctuation / 34
display : 156
punctuation : 25
display ; 56
punctuation ; 23
display < 126
math < 5-246
display = 123456
math = 5-2356
display > 345
punctuation > 5-135
display @ 47
sign @ 4
display [ 2467
punctuation [ 46-126
