display ! 2346
punctuation ! 235

display " 5
punctuation " 236

display ? 1456
punctuation ? 26
match %[^_~]%<* " %[_.$]*%[a#] 236
match %[a#]%[_.$]* " %>*%[^_~] 356

display # 3456
sign # 3456
display $ 1246
sign $ 4-234
display % 146
sign % 3456-245-356
math % 3456-245-356
display & 12346
sign & 12346
punctuation ' 3
display ( 12356
punctuation ( 4-126
display ) 23456
punctuation ) 4-345
display * 16
punctuation * 236
