  <!-- Description of the double-tap action. Used to populate "Double-tap to activate" hint. -->
  <string name="value_double_tap">Double-tap</string>
  <!-- Description of the single-tap action. Used to populate "Tap to activate" hint. -->
  <string name="value_single_tap">Tap</string>
  <!-- Description of the selection action (for Switch Access). Used to populate "Press the Select Switch to activate" hint. -->
  <string name="value_switch_press" translatable="false">Press the Select Switch</string>
  <!--
    Description of the action to press select on a TV remote. Used to populate "Press select to
    activate" hint. The terminology for the key should match the wording used for the center
    button of the directional pad on Android TV remotes; a longer translation equivalent to
    "Press select key" is also OK.
    This is spoken to the user and will be filled in as part of a template, e.g. "X to activate"
    or "X to toggle".
  -->
  <string name="value_press_select">Press select</string>
  <!-- Hint spoken when the user pauses over an item that can be clicked. Populated with an action like "Tap" or "Double-tap". -->
  <string name="template_hint_clickable"><xliff:g id="action" example="Tap">%1$s</xliff:g> to activate.</string>
  <!-- Hint spoken when the user pauses over an item that can be clicked while they are navigating with keyboard. -->
  <string name="template_hint_clickable_keyboard">Press <xliff:g id="keyCombo" example="Alt+Enter">%1$s</xliff:g> to activate.</string>
