<?xml version="1.0" encoding="UTF-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:key="@string/pref_category_manage_gestures_key">
  <com.google.android.accessibility.utils.preference.AccessibilitySuitePreference
      android:title="@string/practice_gestures_title"
      android:key="@string/pref_practice_gestures_entry_point_key" />
  <com.google.android.accessibility.utils.preference.AccessibilitySuitePreference
      android:title="@string/reset_gesture_settings_title"
      android:key="@string/pref_reset_gesture_settings_key"
      app:allowDividerBelow="true"/>
