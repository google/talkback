      android:key="@string/pref_gesture_set_key"
      android:title="@string/pref_gesture_set_key_title"
      android:summary="%s"
      android:defaultValue="@string/pref_gesture_set_value_default"
      android:entries="@array/pref_gesture_set_entries"
      android:entryValues="@array/pref_gesture_set_values" />
